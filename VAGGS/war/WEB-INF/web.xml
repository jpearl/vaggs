<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
	   <servlet-name>PostRoute</servlet-name>
	   <servlet-class>com.vaggs.Servlets.PostRouteServ</servlet-class>
	</servlet>
	<servlet>
       <servlet-name>Tower</servlet-name>
       <servlet-class>com.vaggs.Servlets.TowerServ</servlet-class>
    </servlet>
	<servlet>
       <servlet-name>Route</servlet-name>
      <servlet-class>com.vaggs.Servlets.RouteQueryServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChannelRegistration</servlet-name>
        <servlet-class>com.vaggs.Servlets.ChannelRegistration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChannelConnection</servlet-name>
        <servlet-class>com.vaggs.Servlets.ChannelConnection</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AirportInfo</servlet-name>
        <servlet-class>com.vaggs.Servlets.AirportInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SavedRoutes</servlet-name>
        <servlet-class>com.vaggs.Servlets.SavedRoutes</servlet-class>
    </servlet>
	<servlet-mapping>
		<servlet-name>PostRoute</servlet-name>
		<url-pattern>/postroute</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
        <servlet-name>SavedRoutes</servlet-name>
        <url-pattern>/savedroutes</url-pattern>
    </servlet-mapping>
	<servlet-mapping>
        <servlet-name>Route</servlet-name>
        <url-pattern>/route</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Tower</servlet-name>
        <url-pattern>/tower</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChannelRegistration</servlet-name>
        <url-pattern>/channelregistration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChannelConnection</servlet-name>
        <url-pattern>/_ah/channel/connected/</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChannelConnection</servlet-name>
        <url-pattern>/_ah/channel/disconnected/</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AirportInfo</servlet-name>
        <url-pattern>/airportinfo</url-pattern>
    </servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
 <servlet>
  <servlet-name>SystemServiceServlet</servlet-name>
  <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
  <init-param>
   <param-name>services</param-name>
   <param-value/>
  </init-param>
 </servlet>
 <servlet-mapping>
  <servlet-name>SystemServiceServlet</servlet-name>
  <url-pattern>/_ah/spi/*</url-pattern>
 </servlet-mapping>
</web-app>